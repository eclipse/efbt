grammar org.eclipse.emf.eson.xtextintegration.MyDsl with org.eclipse.xtext.common.Terminals

generate myDsl "http://www.eclipse.org/emf/eson/xtextintegration/MyDsl"

Model:
	'Greetings' name=ID
	('REF' refModel = [Model])?
	('REFs' (refModels += [Model])+)?
	greetings+=Greeting*;
	
Greeting:
	'Hello' name=ID '!';
